@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_MainNoLayout.cshtml";
}
<link href="/Css/RemoteConfiguration/Create.css" rel="stylesheet" type="text/css" />
<link href="/Css/Share/Share.css" rel="stylesheet" type="text/css" />
<form action="/RemoteConfiguration/Create" id="formCreate" method="post" name="formCreate" enctype="multipart/form-data" class="easyui-form">
    <div class="from-config">
        <div class="editor-label">
            <label for="Application">
                应用程序:
            </label>
        </div>
        <div class="editor-field">
            <input class="watermark easyui-textbox easyui-validatebox" type="text" id="application" name="Application" value="General" required="true" missingmessage="必填" />
        </div>
        <div class="editor-label">
            <label for="SectionName">
                配置名称 :
            </label>
        </div>
        <div class="editor-field">
            <input class="easyui-textbox easyui-validatebox" type="text" name="SectionName"
                   id="sectionName" required="true" missingmessage="必填" />
        </div>
        <div class="editor-label">
            <label for="fileName">
                配置文件:
            </label>
        </div>
        <div class="editor-field">
            <input type="file" name="fileName" id="fileName" class="easyui-validatebox" required="true" missingmessage="必填" />
        </div>
        <div class="submit-label">
            <div class="editor-field" border="false" style="text-align: center; height: 30px;
            line-height: 30px;">
                <a class="easyui-linkbutton" iconcls="icon-ok" href="javascript:void(0)" id="formSubmit">
                    保存
                </a> <a class="easyui-linkbutton" iconcls="icon-cancel" href="javascript:void(0)"
                        id="formCancel">取消</a>
            </div>
        </div>
    </div>
</form>
<script src="/Js/RemoteConfiguration/Create.js" type="text/javascript"></script>